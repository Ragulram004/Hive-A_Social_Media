{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/users", "dest": "/server.js" },
    { "src": "/api/posts", "dest": "/server.js" },
    { "src": "/api/messages", "dest": "/server.js" },
    { "src": "/(.*)", "dest": "/server.js" }
  ]
}
